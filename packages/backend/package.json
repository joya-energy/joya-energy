{
  "name": "joya-backend",
  "version": "1.0.0",
  "description": "Joya backend server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc-watch -b ./tsconfig.app.json --onSuccess \"node -r ts-node/register -r tsconfig-paths/register ./src/index.ts\"",
    "start": "npm run start:ts ./src/index.ts",
    "start:ts": "node -r ts-node/register -r tsconfig-paths/register",
    "build": "tsc -b ./tsconfig.app.json",
    "start:prod": "node -r tsconfig-paths/register dist/packages/backend/src/index.js",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:w": "jest --runInBand --detectOpenHandles --watch",
    "setup-email": "node -r ts-node/register -r tsconfig-paths/register ./src/scripts/setup-email-dev.ts",
    "script-example": "node -r ts-node/register -r tsconfig-paths/register ../../dist/packages/backend/src/scripts/example-script.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.3",
    "axios": "^1.13.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-promise-router": "^4.1.1",
    "helmet": "^8.1.0",
    "http": "^0.0.1-security",
    "loadsh": "^0.0.4",
    "luxon": "^3.7.2",
    "mongoose": "^8.12.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "openai": "^6.9.1",
    "pdf-to-png-converter": "^3.11.0",
    "sharp": "^0.34.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uniq": "^1.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/sharp": "^0.31.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
